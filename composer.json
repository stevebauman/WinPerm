{
    "name": "stevebauman/winperm",
    "type": "project",
    "description": "A Windows File / Folder Permission Parser.",
    "keywords": [
        "permissions",
        "windows",
        "windows permissions",
        "windows acl"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/stevebauman/WinPerm/issues",
        "source": "https://github.com/stevebauman/WinPerm"
    },
    "authors": [
        {
            "name": "Steve Bauman",
            "email": "steven_bauman@outlook.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.6.*",
        "mockery/mockery": "0.9.*"
    },
    "archive": {
        "exclude": ["/examples", "/tests"]
    },
    "autoload": {
        "psr-4": {
            "Stevebauman\\WinPerm\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stevebauman\\WinPerm\\Tests\\": "tests/"
        }
    }
}
